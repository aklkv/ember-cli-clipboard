{
  "name": "docs-app",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for docs-app goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "vite",
    "test": "vite build --mode development && testem ci"
  },
  "dependencies": {
    "@shikijs/rehype": "^3.7.0",
    "change-case": "^5.4.4",
    "content-tag": "^4.0.0",
    "ember-cli-clipboard": "workspace:*",
    "ember-primitives": "^0.34.0",
    "highlight.js": "^11.11.1",
    "highlightjs-glimmer": "^2.2.2",
    "kolay": "^3.9.1",
    "shiki": "^3.7.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/runtime": "^7.27.6",
    "@ember/app-tsconfig": "^1.0.3",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.2.2",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/compat": "^4.1.0",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.1.1",
    "@embroider/macros": "^1.18.0",
    "@embroider/router": "^3.0.1",
    "@embroider/vite": "^1.1.5",
    "@eslint/js": "^9.30.1",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "^2.0.0-alpha.2",
    "@glint/environment-ember-loose": "^2.0.0-alpha.2",
    "@glint/environment-ember-template-imports": "^2.0.0-alpha.2",
    "@glint/template": "^1.6.0-alpha.1",
    "@glint/tsserver-plugin": "^2.0.0-alpha.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/qunit": "^2.19.12",
    "@types/rsvp": "^4.0.9",
    "babel-plugin-ember-template-compilation": "^3.0.0",
    "concurrently": "^9.2.0",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~6.5.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-deprecation-workflow": "^3.4.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.5.0",
    "ember-template-lint": "^7.9.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.21.0",
    "eslint-plugin-qunit": "^8.2.2",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.0.6",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.21.1",
    "stylelint-config-standard": "^38.0.0",
    "testem": "^3.16.0",
    "tracked-built-ins": "^4.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
